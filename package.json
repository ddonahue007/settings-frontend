{
  "name": "settings-frontend",
  "version": "1.1.0",
  "private": false,
  "dependencies": {
    "@babel/compat-data": "^7.13.15",
    "@babel/runtime": "^7.13.17",
    "@data-driven-forms/pf4-component-mapper": "^3.6.0",
    "@data-driven-forms/react-form-renderer": "^3.6.0",
    "@patternfly/patternfly": "^4.102.2",
    "@patternfly/react-core": "^4.115.2",
    "@patternfly/react-table": "^4.26.7",
    "@redhat-cloud-services/frontend-components": "3.1.10",
    "@redhat-cloud-services/frontend-components-notifications": "^3.1.0",
    "@redhat-cloud-services/frontend-components-utilities": "3.1.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "classnames": "^2.3.1",
    "js-yaml": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-promise-middleware": "^6.1.2"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/entry.js",
      "<rootDir>/src/entry-dev.js",
      "<rootDir>/src/logout.js"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "1.1.0",
    "@redhat-cloud-services/frontend-components-config": "4.0.28",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.23.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "stylelint": "^13.13.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "webpack-bundle-analyzer": "^4.4.1"
  },
  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js",
    "test": "jest --verbose",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "prod": "NODE_ENV=production webpack serve --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy",
    "start:beta": "NODE_ENV=development BETA=true webpack serve --config config/dev.webpack.config.js"
  },
  "insights": {
    "appname": "applications"
  }
}
